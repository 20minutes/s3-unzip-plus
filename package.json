{
  "name": "@20minutes/s3-unzip-plus",
  "version": "1.3.1",
  "description": "Utility for unzipping a compressed (application/zip) file from a S3 bucket's root directory back to the same bucket.",
  "keywords": [
    "s3",
    "zip",
    "archive",
    "unzip"
  ],
  "main": "lib/index.js",
  "module": "esm/index.js",
  "exports": {
    ".": {
      "types": "./lib/index.d.ts",
      "import": "./esm/index.js",
      "require": "./lib/index.js"
    }
  },
  "bin": {
    "s3-unzip-plus": "bin/s3-unzip-plus"
  },
  "scripts": {
    "build": "yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && node -e \"require('fs').writeFileSync('esm/package.json', '{\\n  \\\"type\\\": \\\"module\\\"\\n}\\n')\"",
    "clean": "rimraf ./dist ./lib ./esm",
    "build-and-test": "yarn clean && yarn build && yarn test",
    "test": "jest --silent",
    "test:watch": "jest --watch",
    "lint": "eslint src/ tests/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/20minutes/s3-unzip-plus.git"
  },
  "author": "20 Minutes",
  "license": "MIT",
  "engines": {
    "node": ">=22"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.983.0",
    "@aws-sdk/lib-storage": "^3.983.0",
    "adm-zip": "^0.5.16",
    "commander": "^14.0.3",
    "md5": "^2.2.1",
    "mime-types": "^3.0.2"
  },
  "devDependencies": {
    "@20minutes/eslint-config": "^2.0.0",
    "@smithy/util-stream": "^4.5.12",
    "@types/adm-zip": "^0.5.5",
    "@types/jest": "^30.0.0",
    "@types/md5": "^2.3.5",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^25.2.1",
    "@typescript-eslint/parser": "^8.19.1",
    "aws-sdk-client-mock": "^4.1.0",
    "aws-sdk-client-mock-jest": "^4.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "jest": "^30.2.0",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "ts-jest": "^29.2.5",
    "typescript": "^5.4.5"
  },
  "types": "lib/index.d.ts"
}
